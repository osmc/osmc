msgctxt "#10636"
msgid "Use HDMI AVI signalling"
msgstr "Använd HDMI AVI signalering"

msgctxt "#13050"
msgid "Low battery, please replace"
msgstr "Låg batterinivå, byte rekommenderas"

msgctxt "#13133"
msgid "Force RGB output"
msgstr "Tvinga RGB signal"

msgctxt "#13134"
msgid "Lock HDMI HPD"
msgstr "Lås HDMI HPD"

msgctxt "#13135"
msgid "Mute HDMI audio output"
msgstr "Tysta HDMI ljudsignal"

msgctxt "#13136"
