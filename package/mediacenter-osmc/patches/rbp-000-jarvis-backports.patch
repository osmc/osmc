+        {
+          if (stereo_view == RENDER_STEREO_VIEW_LEFT)
+            m_sourceRect.y1 += m_sourceRect.y2*0.5f;
+          else if(stereo_view == RENDER_STEREO_VIEW_RIGHT)
+            m_sourceRect.y2 *= 0.5f;
+        }
+        else
+        {
+          if (stereo_view == RENDER_STEREO_VIEW_LEFT)
+            m_sourceRect.y2 *= 0.5f;
