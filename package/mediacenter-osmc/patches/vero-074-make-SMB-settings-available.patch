From c80ff5d8ac027c1a1ed8c5dfb8867ec502277ade Mon Sep 17 00:00:00 2001
From: Sam Nazarko <email@samnazarko.co.uk>
Date: Fri, 8 Sep 2017 03:33:07 +0100
Subject: [PATCH] Make SMB settings available for iMX6

Signed-off-by: Sam Nazarko <email@samnazarko.co.uk>
---
 system/settings/imx6.xml | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/system/settings/imx6.xml b/system/settings/imx6.xml
index 3af9b3f..9cfe132 100644
--- a/system/settings/imx6.xml
+++ b/system/settings/imx6.xml
@@ -28,4 +28,13 @@
       </group>
     </category>
   </section>
+  <section id="services">
+    <category id="smb">
+      <group id="2">
+        <setting id="smb.maxprotocol">
+          <visible>true</visible>
+        </setting>
+      </group>
+    </category>
+  </section>
 </settings>
