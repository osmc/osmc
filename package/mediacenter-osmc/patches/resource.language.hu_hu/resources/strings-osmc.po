msgctxt "#40050"
msgid "Low battery, please charge"
msgstr "Alacsony töltöttségi szint. Kérjük, cserélje ki az akkumulátort vagy töltse fel"

msgctxt "#40051"
msgid "Battery is charging"
msgstr "Az akkumulátor töltődik"

msgctxt "#40052"
msgid "Battery is fully charged"
msgstr "Az akkumulátor teljesen feltöltésre került"

msgctxt "#40053"
msgid "Battery charger removed"
msgstr "Az akkumulátortöltő eltávolításra került"

msgctxt "#40054"
