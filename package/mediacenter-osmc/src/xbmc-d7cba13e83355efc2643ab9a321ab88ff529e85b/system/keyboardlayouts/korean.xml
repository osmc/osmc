<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
Please use English language names instead.
Default font lacks support for all characters
-->
<keyboardlayouts>
  <layout language="Korean" layout="ㄱㄴㄷ" codingtable="Korean">
    <keyboard>
      <row>0123456789</row>
      <row>ㄱㄴㄷㄹㅁㅂㅅㅇㅈㅊㅋ</row>
      <row>ㅌㅍㅎㄲㄸㅃㅆㅉㅏㅑㅓ</row>
      <row>ㅕㅗㅛㅜㅠㅡㅣㅐㅔㅒㅖ</row>
    </keyboard>
    <keyboard modifiers="shift">
      <row>0123456789</row>
      <row>ㄱㄴㄷㄹㅁㅂㅅㅇㅈㅊㅋ</row>
      <row>ㅌㅍㅎㄲㄸㅃㅆㅉㅏㅑㅓ</row>
      <row>ㅕㅗㅛㅜㅠㅡㅣㅐㅔㅒㅖ</row>
    </keyboard>
    <keyboard modifiers="symbol,shift+symbol">
      <row>)!@#$%^&amp;*(</row>
      <row>[]{}-_=+;:</row>
      <row>'",.&lt;&gt;/?\|</row>
      <row>`~</row>
   </keyboard>
  </layout>
</keyboardlayouts>
