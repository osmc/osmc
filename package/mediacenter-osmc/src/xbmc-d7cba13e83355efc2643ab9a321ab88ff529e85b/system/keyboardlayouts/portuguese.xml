<keyboardlayouts>
  <layout language="Portuguese (Portugal)" layout="QWERTY">
    <keyboard>
      <row>\1234567890'</row>
      <row>qwertyuiop+´</row>
      <row>asdfghjklçº~</row>
      <row>&lt;zxcvbnm,.-</row>
    </keyboard>
    <keyboard modifiers="shift">
      <row>|1234567890?</row>
      <row>QWERTYUIOP*`</row>
      <row>ASDFGHJKLÇª^</row>
      <row>&gt;ZXCVBNM;:_</row>
    </keyboard>
    <keyboard modifiers="symbol,shift+symbol">
      <row>|!"#$%&amp;/()=?</row>
      <row>@£§{[]}€áãâà</row>
      <row>éêíóõôúÁÃÂÀÉ</row>
      <row>ÊÍÓÕÔÚ«»¨</row>
    </keyboard>
  </layout>
</keyboardlayouts>
