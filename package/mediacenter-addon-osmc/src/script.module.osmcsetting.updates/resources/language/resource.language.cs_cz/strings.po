msgctxt "#32046"
msgid "Suppress on-screen progress updates"
msgstr "Potlačit aktualizace průběhu na obrazovce"

msgctxt "#32053"
msgid "Install available updates Now"
msgstr "Instalovat dostupné aktualizace ihned"

msgctxt "#32056"
msgid "Scan for updates now"
msgstr "Zkontrolovat aktualizace ihned"

msgctxt "#32057"
msgid "Prompt for all actions"
msgstr "Upozornit na všechny akce"

msgctxt "#32058"
msgid "Display icon on home screen only"
msgstr "Pouze zobrazit ikonu na výchozí obrazovce"

msgctxt "#32059"
msgid "When updating..."
msgstr "Při aktualizaci..."

msgctxt "#32060"
msgid "Download and display icon"
msgstr "Stáhnout a zobrazit ikonu"

msgctxt "#32061"
msgid "Download and install automatically"
msgstr "Stáhnout a instalovat automaticky"

msgctxt "#32062"
msgid "Download, install, auto-restart if needed"
msgstr "Stáhnout, instalovat a automaticky restartovat pokud je třeba"

msgctxt "#32063"
msgid "Do not prompt, display icon, or take any action"
msgstr "Neupozorňovat, nezobrazovat ikonu ani neprovádět žádnou akci"

msgctxt "#32064"
msgid "Action when updates are found"
msgstr "Při aktualizaci byly nalezeny akce"

msgctxt "#32065"
msgid "Only show prompts on the Home screen"
msgstr "Pouze zobrazit upozornění na hlavní obrazovce"

msgctxt "#32066"
msgid "Icon Position Horizontal (% of screen)"
msgstr "Vodorovná pozice ikony (% z obrazovky)"

msgctxt "#32067"
msgid "Icon Position Vertical (% of screen)"
msgstr "Svislá pozice ikony (% z obrazovky)"

msgctxt "#32068"
msgid "Do not show icon on home screen"
msgstr "Nezobrazovat ikonu na hlavní obrazovce"

msgctxt "#32069"
msgid "Only check for updates if system is idle for 1 minute"
msgstr "Aktualizace kontrolovat pouze pokud je systém 1 minutu nečinný"

msgctxt "#32070"
msgid "Do not check for updates during playback"
msgstr "Nekontrolovat aktualizace během přehrávání"

msgctxt "#32071"
msgid "Delay (minutes)"
msgstr "Zpoždění (minut)"

msgctxt "#32072"
msgid "OSMC Update Available"
msgstr "Dostupná Aktualizace OSMC"

msgctxt "#32073"
msgid "Updates have been downloaded and can be installed immediately."
msgstr "Aktualizace byla stažena a bude neprodleně nainstalována."

msgctxt "#32074"
msgid "Would you like to install the updates now?"
msgstr "Chcete provést instalaci aktualizací nyní?"

msgctxt "#32075"
msgid "Updates have been downloaded, but Kodi will need to exit to install them."
msgstr "Aktualizace byly staženy, ale Kodi se bude muset ukončit pro jejich nainstalování."

msgctxt "#32076"
msgid "Would you like to exit and install the updates now?"
msgstr "Chcete ukončit Kodi a provést instalaci aktualizací nyní?"

msgctxt "#32077"
msgid "OSMC Updater"
msgstr "Aktualizace OSMC"

msgctxt "#32078"
msgid "Update Running."
msgstr "Aktualizace probíhá."

msgctxt "#32079"
msgid "Packages have been installed that require a reboot."
msgstr "Nainstalované balíčky vyžadují restart."

msgctxt "#32080"
msgid "Would you like to reboot now?"
msgstr "Chcete nyní provést restart?"

msgctxt "#32081"
msgid "Reboot Now"
msgstr "Restartovat nyní"

msgctxt "#32082"
msgid "Reboot Later"
msgstr "Restartovat později"

msgctxt "#32083"
msgid "There are updates that are available for install."
msgstr "Jsou k dispozici aktualizace, které je možné nainstalovat."

msgctxt "#32084"
msgid "Would you like to install them now?"
msgstr "Chcete je nainstalovat nyní?"

msgctxt "#32085"
msgid "Fair enough, then."
msgstr "Dost spravedlivé."

msgctxt "#32086"
msgid "You can install them from within the OSMC settings later."
msgstr "Můžete je nainstalovat z nastavení OSMC později."

msgctxt "#32087"
msgid "OSMC Update Error"
msgstr "Chyba aktualizace OSMC"

msgctxt "#32088"
msgid "Error installing: %s "
msgstr "Chyba instalace: %s "

msgctxt "#32089"
msgid "Please report this on the OSMC forum."
msgstr "Nahlaste to prosím na fóru OSMC."

msgctxt "#32090"
msgid "Update"
msgstr "Aktualizace"

msgctxt "#32091"
msgid "Operations successfully completed."
msgstr "Operace úspěšně dokončena."

msgctxt "#32092"
msgid "There are no updates available."
msgstr "Žádné dostupné aktualizace."

msgctxt "#32093"
msgid "Update Settings"
msgstr "Nastavení aktualizací"

msgctxt "#32094"
msgid "Manual Controls"
msgstr "Ruční ovládání"

msgctxt "#32095"
msgid "Download updates, then prompt"
msgstr "Stáhnout aktualizace a poté upozornit"

msgctxt "#32096"
msgid "Backup"
msgstr "Záloha"

msgctxt "#32097"
msgid "Backup error. Details in log."
msgstr "Chyba zálohování. Podrobnosti v logu."

msgctxt "#32098"
msgid "Items successfully restored."
msgstr "Položky byly úspěšně obnoveny."

msgctxt "#32099"
msgid "Restoring guisettings.xml requires Kodi to restart."
msgstr "Obnovení souboru guisettings.xml vyžaduje restart Kodi."

msgctxt "#32100"
msgid "Restart Now"
msgstr "Restartovat ihned"

msgctxt "#32101"
msgid "Restart Later"
msgstr "Restartovat později"

msgctxt "#32102"
msgid "Error"
msgstr "Chyba"

msgctxt "#32103"
msgid "Unsupported Alphas cannot be upgraded."
msgstr "Nepodporované Alfa verze nemohou být aktualizovány."

msgctxt "#32104"
msgid "Please install the latest version of OSMC."
msgstr "Prosím nainstalujte poslední verzi OSMC."

msgctxt "#32105"
msgid "Run Backup of settings now."
msgstr "Spustit nyní zálohu nastavení."

msgctxt "#32106"
msgid "Restore a backup of settings now."
msgstr "Obnovit nyní zálohu nastavení."

msgctxt "#32107"
msgid "Backup before each update (excludes manual update)."
msgstr "Provést zálohu před každou aktualizací (kromě ruční aktualizace)."

msgctxt "#32108"
msgid "Number of backup files to keep (0 for keep all)"
msgstr "Počet uchovávaných souborů se zálohou (0 zachovat vše)"

msgctxt "#32109"
msgid "Backups"
msgstr "Zálohy"

msgctxt "#32110"
msgid "OSMC Restore"
msgstr "Obnovení OSMC"

msgctxt "#32111"
msgid "Minute"
msgstr "Minut"

