msgid "Backup error. Details in log."
msgstr "Errore di backup. Dettagli nel log."

msgctxt "#32098"
msgid "Items successfully restored."
msgstr "Elemento ripristinato con successo."

msgctxt "#32099"
msgid "Restoring guisettings.xml requires Kodi to restart."
msgstr "Ripristinare guisettings.xml richiede il riavvio di Kodi."

msgctxt "#32100"
msgid "Restart Now"
msgstr "Riavvia Adesso"

msgctxt "#32101"
msgid "Restart Later"
msgstr "Riavvia Dopo"

msgctxt "#32102"
msgid "Error"
msgstr "Errore"

msgctxt "#32103"
msgid "Unsupported Alphas cannot be upgraded."
msgstr "Le Alpha non supportate non possono essere aggiornate."

msgctxt "#32104"
msgid "Please install the latest version of OSMC."
msgstr "Installa l'ultima versione di OSMC."

msgctxt "#32105"
msgid "Run Backup of settings now."
msgstr "Esegui un backup delle impostazioni adesso."

msgctxt "#32106"
msgid "Restore a backup of settings now."
msgstr "Ripristina un backup delle impostazioni adesso."

msgctxt "#32107"
msgid "Backup before each update (excludes manual update)."
msgstr "Backup prima di ogni aggiornamento (esclusi aggiornamenti manuali)."

msgctxt "#32108"
msgid "Number of backup files to keep (0 for keep all)"
msgstr "Numero dei file di backup da mantenere (0 per tenerli tutti)"

msgctxt "#32109"
msgid "Backups"
msgstr "Backup"

